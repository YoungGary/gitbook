{
    "title": "Gary的小窝",
    "author": "Gary",
    "description": "Gary的小窝. 里面包含了个人撰写的技术文章",
    "language": "zh-hans",
    "styles": {
  
    },
    "plugins": [
      "theme-comscore",
      "prism",
      "-highlight",
      "copy-code-button",
      "search-pro",
      "-search",
      "-lunr",
      "expandable-chapters",
      "splitter",
      "-sharing",
      "tbfed-pagefooter",
    
      "anchor-navigation-ex"
    ],
    "pluginsConfig": {
      "prism": {
        "css": [
          "prismjs/themes/prism-solarizedlight.css"
        ],
        "lang": {
          "shell": "bash"
        }
      },
      "tbfed-pagefooter": {
        "copyright":"Copyright &copy YoungGary 2019",
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
      },
      
      "anchor-navigation-ex": {
        "showLevel": false
      }
    }
  }